\newcommand\ctverec[1]{
  \dimen0=#1
  \mathop{ 			% operátor čtvereček
    \mkern 2mu
    \vbox{\hrule height0.05\dimen0\hbox{\vrule height#1 width0.05\dimen0\hskip#1\vrule width0.14\dimen0}\hrule height0.14\dimen0}
  }
  \mkern 2mu
}
\renewcommand\Dalembert{\mathchoice			% operátor D'Alembert
  {\ctverec{5.3pt}}
  {\ctverec{5.3pt}}
  {\ctverec{3.5pt}}
  {\ctverec{2.5pt}}
}
